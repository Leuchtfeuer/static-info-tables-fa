            }

            SJBR\StaticInfoTables\Domain\Model\Language {
                mapping {
                    columns {
                        lg_name_fa.mapOnProperty = nameFa
                    }
                }
            }

            SJBR\StaticInfoTables\Domain\Model\Territory {
                mapping {
                    columns {
                        tr_name_fa.mapOnProperty = nameFa
                    }
                }
            }
        }
