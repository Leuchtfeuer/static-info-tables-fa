                mapping {
                    columns {
                        cn_short_fa.mapOnProperty = shortNameFa
                    }
                }
            }

            SJBR\StaticInfoTables\Domain\Model\CountryZone {
                mapping {
                    columns {
                        zn_name_fa.mapOnProperty = nameFa
